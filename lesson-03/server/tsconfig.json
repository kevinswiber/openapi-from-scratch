{
  "include": [
    "./src"
  ],
  "compilerOptions": {
    "module": "nodenext",
    "rootDir": "./src",
    "outDir": "./dist",
    "resolvePackageJsonImports": true,
    "verbatimModuleSyntax": true,
    "incremental": true,
    "tsBuildInfoFile": "./.cache/server.tsbuildinfo",
    "strictNullChecks": true
  },
  "watchOptions": {
    "watchDirectory": "dynamicPriorityPolling",
    "watchFile": "dynamicPriorityPolling"
  }
}
